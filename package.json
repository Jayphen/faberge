{
  "name": "my-razzle-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "check-types": "tsc",
    "codegen": "yarn schema && apollo codegen:generate --target=typescript '--includes=src/**/*.tsx' --tagName=gql --localSchemaFile=./graphql-schema.json --globalTypesFile=./src/graphql-global-types.ts --addTypename",
    "schema": "apollo schema:download --endpoint=http://localhost:8080/v1alpha1/graphql graphql-schema.json"
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@types/node-fetch": "^2.1.6",
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.4.13",
    "apollo-env": "^0.3.3",
    "apollo-link": "^1.2.8",
    "apollo-link-http": "^1.5.11",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "normalize.css": "^8.0.1",
    "razzle": "next",
    "razzle-plugin-typescript": "next",
    "react": "^16.8.3",
    "react-apollo-hooks": "^0.4.1",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack-env": "^1.13.7",
    "@typescript-eslint/eslint-plugin": "^1.4.1",
    "@typescript-eslint/parser": "^1.4.1",
    "apollo": "^2.5.1",
    "babel-plugin-emotion": "^10.0.7",
    "browserslist": "^4.4.2",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.2.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  }
}
