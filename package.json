{
  "name": "my-razzle-app",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "check-types": "tsc",
    "codegen": "yarn schema && apollo codegen:generate --target=typescript '--includes=src/**/*.tsx' --tagName=gql --localSchemaFile=./graphql-schema.json --globalTypesFile=./src/graphql-global-types.ts --addTypename",
    "schema": "graphql get-schema",
    "format:prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}' '.editorconfig' 'LICENSE'",
    "display-browser-support": "browserslist --stats && echo '' && browserslist --coverage && echo '' && echo 'See more at https://browserl.ist/'"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js"
    }
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.7",
    "@emotion/core": "^10.0.7",
    "@types/node-fetch": "^2.1.6",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-env": "^0.3.4",
    "apollo-link": "^1.2.9",
    "apollo-link-http": "^1.5.12",
    "express": "^4.16.4",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "normalize.css": "^8.0.1",
    "razzle": "next",
    "razzle-plugin-typescript": "next",
    "react": "^16.8.4",
    "react-apollo-hooks": "^0.4.3",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.0",
    "@types/react": "^16.8.7",
    "@types/react-dom": "^16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "apollo": "^2.6.0",
    "babel-plugin-emotion": "^10.0.7",
    "browserslist": "^4.4.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-typescript": "^2.0.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-graphql": "^3.0.3",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "graphql-cli": "^3.0.11",
    "husky": "^1.3.1",
    "jest-dom": "^3.1.2",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.0",
    "typescript": "^3.3.3333"
  }
}
